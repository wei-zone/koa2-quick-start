{
    "name": "koa2-quick-start",
    "version": "1.0.0",
    "description": "koa2-quick-start",
    "keywords": [
        "koa2",
        "koa2-ts",
        "koa2-template",
        "koa2-quick-start",
        "koa2-ts-quick-start"
    ],
    "author": "wforguo",
    "homepage": "https://forguo.cn",
    "engines": {
        "node": ">=16.14.0"
    },
    "packageManager": "pnpm@7.6.0",
    "scripts": {
        "dev": "nodemon src/main.ts",
        "start": "cross-env NODE_ENV=production ts-node src/main.ts",
        "start:dist": "cross-env NODE_ENV=production nodemon src/main.js",
        "pm2": "cross-env NODE_ENV=production pm2 start ./src/main.js --watch",
        "build": "cross-env NODE_ENV=production tsc --build",
        "automate": "sequelize-automate -c automate.config.json && npm run format",
        "coverage": "nyc --reporter lcov npm run test",
        "lint": "npm run lint-json && npm run lint-ts",
        "lint-json": "eslint --cache --fix --no-eslintrc -c .eslintrc-json.json --ext .json .",
        "lint-ts": "tsc --noEmit && eslint --cache --fix --ext .cjs,.js,.jsx,.mjs,.ts,.tsx .",
        "format": "prettier --write '**/*' --ignore-unknown",
        "prepare": "husky install",
        "test": "mocha -r ts-node/register --check-leaks --parallel src/**/*.test.ts"
    },
    "dependencies": {
        "@cloudbase/node-sdk": "^2.11.0",
        "@koa/router": "^12.0.0",
        "axios": "^1.6.7",
        "chalk": "^4.1.2",
        "config": "^3.3.7",
        "cos-nodejs-sdk-v5": "^2.14.4",
        "dayjs": "^1.11.9",
        "jsonwebtoken": "^9.0.2",
        "koa": "^2.13.4",
        "koa-body": "^4.2.0",
        "koa-compress": "^5.1.0",
        "koa-helmet": "^7.0.2",
        "koa-json": "^2.0.2",
        "koa-jwt": "^4.0.4",
        "koa-logger": "^3.2.1",
        "koa-range": "^0.3.0",
        "koa-response-time": "^2.1.0",
        "koa-static": "^5.0.0",
        "koa-websocket": "^7.0.0",
        "koa2-swagger-ui": "^5.9.1",
        "log4js": "^6.9.1",
        "memory-cache": "^0.2.0",
        "module-alias": "^2.2.3",
        "mysql2": "^3.11.0",
        "node-schedule": "^2.1.1",
        "sequelize": "^6.37.3",
        "sequelize-automate": "^1.2.2",
        "socket.io": "^4.7.4",
        "swagger-jsdoc": "^6.2.8",
        "typescript": "^4.7.4",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/chai": "^4.3.0",
        "@types/config": "^3.3.0",
        "@types/koa": "^2.13.4",
        "@types/koa-compose": "^3.2.5",
        "@types/koa-compress": "^4.0.3",
        "@types/koa-json": "^2.0.23",
        "@types/koa-logger": "^3.1.2",
        "@types/koa-range": "^0.3.5",
        "@types/koa-response-time": "^2.1.2",
        "@types/koa-static": "^4.0.2",
        "@types/koa__router": "^12.0.0",
        "@types/memory-cache": "^0.2.3",
        "@types/mocha": "^10.0.1",
        "@types/module-alias": "^2.0.4",
        "@types/node": "^20.4.7",
        "@types/node-schedule": "^2.1.5",
        "@types/sinon": "^10.0.11",
        "@types/supertest": "^2.0.11",
        "@types/swagger-jsdoc": "^6.0.2",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "@vercel/node": "^3.0.5",
        "chai": "^4.3.6",
        "cross-env": "^7.0.3",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-prettier": "^5.0.0",
        "husky": "^8.0.3",
        "koa-compose": "^4.1.0",
        "lint-staged": "^13.2.3",
        "mocha": "^10.2.0",
        "nodemon": "^3.0.1",
        "nyc": "^15.1.0",
        "prettier": "^3.0.0",
        "release-it": "^16.1.3",
        "sinon": "^15.2.0",
        "supertest": "^6.2.2",
        "ts-node": "^10.5.0"
    }
}
